{
  "name": "saltcandy123font-demo-page",
  "author": "saltcandy123 <saltcandy123@gmail.com>",
  "license": "MIT",
  "homepage": ".",
  "scripts": {
    "lint": "prettier -u -c './*' .github public src --ignore-path .gitignore && eslint src",
    "lint:fix": "prettier -u -w './*' .github public src --ignore-path .gitignore && eslint src --fix",
    "dev": "react-scripts start",
    "build": "BUILD_PATH=dist react-scripts build"
  },
  "dependencies": {
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.10.0",
    "@saltcandy123/saltcandy123font": "^0.7.0",
    "@types/node": "^18.7.3",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1",
    "typescript": "^4.7.4"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:react/all",
      "prettier"
    ],
    "rules": {
      "react/jsx-max-depth": "off",
      "react/jsx-no-literals": "off",
      "react/jsx-filename-extension": [
        "error",
        {
          "extensions": [
            ".jsx",
            ".tsx"
          ]
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
