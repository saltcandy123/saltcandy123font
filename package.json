{
  "name": "saltcandy123font-demo-page",
  "author": "saltcandy123 <saltcandy123@gmail.com>",
  "license": "MIT",
  "homepage": ".",
  "scripts": {
    "lint": "prettier -u -c './*' .github public src --ignore-path .gitignore && eslint src",
    "lint:fix": "prettier -u -w './*' .github public src --ignore-path .gitignore && eslint src --fix",
    "dev": "react-scripts start",
    "build": "BUILD_PATH=dist react-scripts build"
  },
  "dependencies": {
    "@emotion/react": "11.11.3",
    "@emotion/styled": "11.11.0",
    "@mui/icons-material": "5.15.5",
    "@mui/material": "5.15.5",
    "@saltcandy123/saltcandy123font": "0.7.1",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "eslint-config-prettier": "9.1.0",
    "prettier": "3.2.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-scripts": "5.0.1",
    "typescript": "5.3.3"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:react/all",
      "prettier"
    ],
    "rules": {
      "react/jsx-max-depth": "off",
      "react/jsx-no-literals": "off",
      "react/jsx-filename-extension": [
        "error",
        {
          "extensions": [
            ".jsx",
            ".tsx"
          ]
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
